<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="297.0" prefWidth="421.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.workslink.NoteController">
   <children>
      <Label layoutX="14.0" layoutY="110.0" prefHeight="18.0" prefWidth="109.0" text="Your Note:" />
      <TextField fx:id="yourNoteTextField" layoutX="14.0" layoutY="149.0" prefHeight="83.0" prefWidth="380.0" styleClass="tx" stylesheets="@../CSS/design.css" />
      <Button fx:id="saveButton" layoutX="169.0" layoutY="248.0" mnemonicParsing="false" onAction="#saveNotes" prefHeight="18.0" prefWidth="82.0" text="Save">
         <stylesheets>
            <URL value="@../CSS/BasicDesign.css" />
            <URL value="@../CSS/design.css" />
         </stylesheets>
         <styleClass>
            <String fx:value="fill-btn" />
            <String fx:value="tx" />
         </styleClass></Button>
      <Label layoutX="14.0" layoutY="28.0" prefHeight="18.0" prefWidth="82.0" text="Note Name:" />
      <TextField fx:id="noteNameTextField" layoutX="14.0" layoutY="58.0" prefHeight="34.0" prefWidth="380.0" styleClass="tx" stylesheets="@../CSS/design.css" />
      <ImageView fitHeight="41.0" fitWidth="45.0" layoutX="366.0" layoutY="8.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Icon/logo.png" />
